  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon -w app --exec \"babel-node app --presets env\" ",
    "build": "cross-env NODE_ENV=production babel app -s -D -d dist --presets env",
    "prod-start": "cross-env NODE_ENV=production pm2 start dist/index.js",
    "prod-stop": "cross-env NODE_ENV=production pm2 stop dist/index.js",
    "prod-restart": "cross-env NODE_ENV=production pm2 restart dist/index.js",
    "preset": "npm run -s build",
    "lint": "eslint app",
    "test": "cross-env NODE_ENV=test mocha --require babel-polyfill --require babel-register",
    "coverage": "cross-env NODE_ENV=test nyc mocha"
